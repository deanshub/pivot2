    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "concurrently": "^3.1.0",
    "css-loader": "^0.23.1",
    "csv-stream": "^0.1.3",
    "eslint": "^3.12.0",
