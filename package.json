{
  "name": "pivot2",
  "version": "1.3.15",
  "description": "An awesome implementaion for pivot table",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "set NODE_ENV=development&&webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 6789",
    "webpack": "webpack -d --inline --progress --colors --watch",
    "server": "nodemon server/app.js",
    "dev": "concurrently \"yarn run server\" \"yarn run webpack\"",
    "build": "set NODE_ENV=production&&webpack -p --progress --colors",
    "build:dev": "set NODE_ENV=development&&webpack --progress --colors",
    "build_linux": "NODE_ENV=production webpack -p --progress --colors",
    "dist": "set NODE_ENV=production&&webpack -p --progress --colors --config dist.webapck.config.js",
    "dist:dev": "set NODE_ENV=development&&webpack --progress --colors --config dist.webapck.config.js",
    "dist:npm": "set NODE_ENV=production&&webpack --progress --colors --config npm.webapck.config.js",
    "dist:npm:dev": "set NODE_ENV=development&&webpack --progress --colors --config npm.webapck.config.js",
    "prepublish": "npm run dist:npm"
  },
  "license": "MIT",
