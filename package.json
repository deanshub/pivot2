{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A boilerplate of things that shouldn't exist",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "set NODE_ENV=development&&webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 6789",
    "webpack": "webpack -d --inline --progress --colors --watch",
    "server": "nodemon --ignore server/static server/app.js",
    "dev": "concurrently \"yarn run webpack\" \"yarn run server\"",
    "build": "set NODE_ENV=production&&webpack -p --progress --colors"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "classnames": "^2.2.3",
    "concurrently": "^3.1.0",
    "css-loader": "^0.23.1",
    "csv-stream": "^0.1.3",
    "eslint": "^3.12.0",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.8.5",
    "postcss-loader": "^0.8.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "rucksack-css": "^0.8.5",
    "rx-http-request": "^1.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "react-document-title": "^2.0.2",
    "request": "^2.79.0",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "superagent": "^3.3.1",
    "worker-loader": "^0.7.1"
  }
